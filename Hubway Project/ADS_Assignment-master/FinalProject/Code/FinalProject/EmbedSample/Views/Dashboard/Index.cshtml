@section Breadcrumbs{
    <ol class="breadcrumb navbar-breadcrumb">
        <li>Dashboard</li>
    </ol>
}

<div class="side-body padding-top">
    @Html.PowerBIAccessToken()
    <div class="row  no-margin-bottom">
        <div class="col-sm-12 col-xs-12">
            <div class="row">
                <div class="col-xs-12">
                    <div class="card primary">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-3 col-sm-12">
                    <div class="thumbnail no-margin-bottom">
                        <img src="../img/thumbnails/Clinton.jpg" class="img-responsive">
                        <div class="caption">
                            <h3 id="thumbnail-label">Hillary Clinton<a class="anchorjs-link" href="#thumbnail-label"><span class="anchorjs-icon"></span></a></h3>
                            <p>Hillary Diane Rodham Clinton is an American politician and the nominee of the Democratic Party for President of the United States in the 2016 election.</p>
                            <p><a  class="btn btn-success" role="button">@Html.ActionLink("Predict(form)", "Webapp", new { candi = 0 }, null)</a> <a  class="btn btn-default" role="button">@Html.ActionLink("Predict(Batch)", "BatchE", new { candi = 4 }, null)</a></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-12">
                    <div class="thumbnail no-margin-bottom">
                        <img src="../img/thumbnails/Berney.jpg" class="img-responsive">
                        <div class="caption">
                            <h3 id="thumbnail-label">Bernie Sanders<a class="anchorjs-link" href="#thumbnail-label"><span class="anchorjs-icon"></span></a></h3>
                            <p>Bernie Sanders is an American politician and was the nominee of the Democratic Party for President of the United States in the 2016 election.</p>
                            <p><a class="btn btn-success" role="button">@Html.ActionLink("Predict(form)", "Webapp", new { candi = 1 }, null)</a> <a class="btn btn-default" role="button">@Html.ActionLink("Predict(Batch)", "BatchE", new { candi = 5 }, null)</a></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-12">
                    <div class="thumbnail no-margin-bottom">
                        <img src="../img/thumbnails/trump.jpg" class="img-responsive">
                        <div class="caption">
                            <h3 id="thumbnail-label">Donald Trump<a class="anchorjs-link" href="#thumbnail-label"><span class="anchorjs-icon"></span></a></h3>
                            <p>Donald Trump is an American businessman and the Republican Party nominee for President of the United States in the 2016 election.</p>
                            <p><a class="btn btn-success" role="button">@Html.ActionLink("Predict(form)", "Webapp", new { candi = 2 }, null)</a> <a class="btn btn-default" role="button">@Html.ActionLink("Predict(Batch)", "BatchE", new { candi = 6 }, null)</a></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-12">
                    <div class="thumbnail no-margin-bottom">
                        <img src="../img/thumbnails/Ted.jpg" class="img-responsive">
                        <div class="caption">
                            <h3 id="thumbnail-label">Ted Cruz<a class="anchorjs-link" href="#thumbnail-label"><span class="anchorjs-icon"></span></a></h3>
                            <p>Rafael Edward "Ted" Cruz is an attorney and was a candidate for the Republican nomination for President of the United States in the 2016 election.</p>
                            <p><a class="btn btn-success" role="button">@Html.ActionLink("Predict(form)", "Webapp", new { candi = 3 }, null)</a> <a class="btn btn-default" role="button">@Html.ActionLink("Predict(Batch)", "BatchE", new { candi = 7 }, null)</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
            <a href="#">
                <div class="card red summary-inline">
                    <div class="card-body">
                        <i class="icon fa fa-inbox fa-4x"></i>
                        <div class="content">
                            <div class="title">GitHub</div>
                            <div class="sub-title">Report, Mockup, InputFile, Usecases and Code</div>
                        </div>
                        <div class="clear-both"></div>
                    </div>
                </div>
            </a>
        </div>
        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
            <a href="https://app.powerbi.com/groups/me/dashboards/eff35e3b-5726-4e88-bb66-98065613e85d" target="_blank">
                <div class="card yellow summary-inline">
                    <div class="card-body">
                        <i class="icon fa fa-comments fa-4x"></i>
                        <div class="content">
                            <div class="title">Real-Time Tweets</div>
                            <div class="sub-title">Power BI login required</div>
                        </div>
                        <div class="clear-both"></div>
                    </div>
                </div>
            </a>
        </div>
        
    </div>
</div>